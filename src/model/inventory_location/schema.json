{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Inventory Location",
  "description": "The Inventory location",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the Inventory location."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The location name."
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The description."
    },
    "image_url": {
      "$id": "#/properties/image_url",
      "type": "string",
      "title": "The url of the image."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the location."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the location was last updated."
    }
  }
}
