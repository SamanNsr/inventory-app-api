{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Item Image",
  "description": "The Item image",
  "required": ["item_id", "image_url"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the company."
    },
    "item_id": {
      "$id": "#/properties/item_id",
      "type": "number",
      "title": "The id of the item."
    },
    "image_url": {
      "$id": "#/properties/image_url",
      "type": "string",
      "title": "The url of the image."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the item."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the item was last updated."
    }
  }
}
