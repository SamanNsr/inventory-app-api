{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Size",
  "description": "The Size",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the company."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The item name."
    },
    "weight": {
      "$id": "#/properties/weight",
      "type": "number",
      "title": "The weight of the item."
    },
    "width": {
      "$id": "#/properties/width",
      "type": "number",
      "title": "The width of the item."
    },
    "height": {
      "$id": "#/properties/height",
      "type": "number",
      "title": "The height of the item."
    },
    "shape_id": {
      "$id": "#/properties/shape_id",
      "type": "number",
      "title": "The id of the shape."
    },
    "volume": {
      "$id": "#/properties/volume",
      "type": "number",
      "title": "The volume of the item."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the item."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the item was last updated."
    }
  }
}
