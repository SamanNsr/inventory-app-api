{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Related Item",
  "description": "The Related item",
  "required": ["item_id", "related_item_id"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the related item."
    },
    "item_id": {
      "$id": "#/properties/item_id",
      "type": "number",
      "title": "The id of the item."
    },
    "related_item_id": {
      "$id": "#/properties/related_item_id",
      "type": "number",
      "title": "The id of the related item."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the related item."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the related item was last updated."
    }
  }
}
