{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Shape",
  "description": "The Shape",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the shape."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The shape name."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the shape."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the shape was last updated."
    }
  }
}
