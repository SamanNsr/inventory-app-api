{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Item Type",
  "description": "The Item type",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the item type."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The item type name."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the item type."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the item type was last updated."
    }
  }
}
