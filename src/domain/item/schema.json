{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Item",
  "description": "The Item",
  "required": ["name", "user_id", "item_type_id", "manufacturer_id"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the company."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The item name."
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "number",
      "title": "The id of the user."
    },
    "item_type_id": {
      "$id": "#/properties/item_type_id",
      "type": "number",
      "title": "The id of the item_type."
    },
    "manufacturer_id": {
      "$id": "#/properties/manufacturer_id",
      "type": "number",
      "title": "The id of the manufacturer."
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The description."
    },
    "size_id": {
      "$id": "#/properties/size_id",
      "type": "number",
      "title": "The id of the size."
    },
    "sku": {
      "$id": "#/properties/sku",
      "type": "string",
      "title": "The sku of the item."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date that the user was last updated."
    }
  }
}
